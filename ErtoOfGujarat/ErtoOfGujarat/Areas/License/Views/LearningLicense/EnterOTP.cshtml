@model ErtoOfGujarat.ResponceMaster
@{
    ViewBag.Title = "EnterOTP";

    string uid = ViewBag.uid;
}

<h2>EnterOTP</h2>

@using (Html.BeginForm("EnterOTP", "LearningLicense", FormMethod.Post, new { @class = "form-horizontal", role = "form" }))
{
    @Html.AntiForgeryToken()

    <div class="form-horizontal">
        <h4>ResponceMaster</h4>
        <hr />
        @Html.ValidationSummary(true, "", new { @class = "text-danger" })

        <div class="form-group">
            @Html.LabelFor(model => model.uniqueId, htmlAttributes: new { @class = "control-label col-md-2" })
            <div class="col-md-10">
                @Html.TextBox("uniqueid", uid)
                @Html.ValidationMessageFor(model => model.uniqueId, "", new { @class = "text-danger" })
            </div>
        </div>

        <div class="form-group">
            @Html.LabelFor(model => model.mOTP, htmlAttributes: new { @class = "control-label col-md-2" })
            <div class="col-md-10">
                @Html.EditorFor(model => model.mOTP, new { htmlAttributes = new { placeholder = "****", @class = "form-control" } })
                @Html.ValidationMessageFor(model => model.aadharNo, "", new { @class = "text-danger" })
            </div>
        </div>

        <div class="form-group">
            @Html.LabelFor(model => model.eOTP, htmlAttributes: new { @class = "control-label col-md-2" })
            <div class="col-md-10">
                @Html.EditorFor(model => model.eOTP, new { htmlAttributes = new { placeholder = "******", @class = "form-control" } })
                @Html.ValidationMessageFor(model => model.aadharNo, "", new { @class = "text-danger" })
            </div>
        </div>


        <div class="form-group">
            <div class="col-md-offset-2 col-md-10">
                <input type="submit" value="Create" class="btn btn-default" />
            </div>
        </div>
    </div>
}