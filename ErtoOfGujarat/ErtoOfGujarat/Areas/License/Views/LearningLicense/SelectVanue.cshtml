@model ErtoOfGujarat.ResponceMaster

@{
    ViewBag.Title = "SelectVanue";
    string uid = ViewBag.uid;
}

<h2>SelectVanue</h2>

@using (Html.BeginForm("SelectVanue", "LearningLicense", FormMethod.Post, new { @class = "form-horizontal", role = "form" }))
{
    @Html.AntiForgeryToken()

    <div class="form-horizontal">
        <h4>Selest Vanue</h4>
        <hr />
        @Html.ValidationSummary(true, "", new { @class = "text-danger" })

        <div class="form-group">
            @Html.LabelFor(model => model.uniqueId, htmlAttributes: new { @class = "control-label col-md-2" })
            <div class="col-md-10">
                @Html.TextBox("uniqueid", uid)
                @*@Html.ValidationMessageFor(model => model.uniqueId, "", new { @class = "text-danger" })*@
            </div>
        </div>

        
            <div class="form-group">
                @Html.Label("vanue","Vanue", htmlAttributes: new { @class = "control-label col-md-2" })
                <div class="col-md-10">
                    @Html.DropDownList("vanue","Select Vanue")
                    @*@Html.ValidationMessageFor(model => model.Duration, "", new { @class = "text-danger" })*@
                </div>
            </div>

        
        <div class="form-group">
            <div class="col-md-offset-2 col-md-10">
                <input type="submit" value="Create" class="btn btn-default" />
            </div>
        </div>
    </div>
}
